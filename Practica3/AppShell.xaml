<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Practica3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Practica3"
    Shell.FlyoutBehavior="Flyout"
    Title="Practica3">
    <FlyoutItem Title="Inicio">
        <ShellContent
    Title="Inicio"
    ContentTemplate="{DataTemplate local:MainPage}"
    Route="MainPage" />
    </FlyoutItem>
    <FlyoutItem Title="Unicornios">
        <ShellContent
        Title="Unicornios"
        ContentTemplate="{DataTemplate local:NewPage1}"
        Route="MainPage" />
    </FlyoutItem>
    <FlyoutItem Title="Fenrir">
        <ShellContent
        Title="Fenrir"
        ContentTemplate="{DataTemplate local:NewPage2}"
        Route="MainPage" />
    </FlyoutItem>
    <FlyoutItem Title="Fenix">
        <ShellContent
        Title="Fenix"
        ContentTemplate="{DataTemplate local:NewPage3}"
        Route="MainPage" />
    </FlyoutItem>
    <FlyoutItem Title="Dragón">
        <ShellContent
    Title="Dragón"
    ContentTemplate="{DataTemplate local:NewPage4}"
    Route="MainPage" />
    </FlyoutItem>


</Shell>
